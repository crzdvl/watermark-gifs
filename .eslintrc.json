{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": [0, 0],
    "strict": 0,
    "no-var": "error",
    "no-console": 1,
    "no-fallthrough": 0,
    "consistent-return": 0,
    "max-len": 0,
    "global-require": 0,
    "no-unused-vars": 0,
    "operator-linebreak": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "prettier/prettier": "error"
  }
}
